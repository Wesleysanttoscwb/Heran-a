package Model;

public class Moto {

	public String cor;
	public int velocidade;
	
	public Moto( String cor, int velocidade) {
		this.cor = cor;
		this.velocidade = velocidade;
	}
	public String getCor() {
		return cor;
		
		
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	
	public int getVelocidade() {
		return velocidade;
	}
	
	public void setVelocidade(int velocidade) {
		this.velocidade = velocidade;
	}

}
